{
  "expo": {
    "name": "TourAndTravelDemo",
    "slug": "tourandtraveldemo",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-av"
    ],
    "description": "This is an experimental/hobby project that's implemented the ReactNative/mobile version of the Web UI idea introduced in this YoutTube video: https://youtu.be/nPZp-YNPyC8",
    "githubUrl": "https://github.com/dev-revelations/TourAndTravel-ReactNative"
  }
}
